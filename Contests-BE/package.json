{
  "name": "contest-be",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "daily": "npm ci && npm run prisma:generate && npm run test:db-connection",
    "build": "npm run clean:dist && tsc",
    "start": "node dist/server.js",
    "dev": "npm run build && nodemon --exec \"ts-node -r tsconfig-paths/register src/server.ts\"",
    "dev:watch": "concurrently \"tsc --watch\" \"nodemon --delay 2s dist/server.js\"",
    "dev:ts": "nodemon --exec \"ts-node -r tsconfig-paths/register src/server.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate --schema src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema src/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema src/prisma/schema.prisma",
    "prisma:seed": "ts-node -r tsconfig-paths/register src/prisma/seed.ts",
    "prisma:reset": "prisma migrate reset --schema src/prisma/schema.prisma --force",
    "prisma:undo-seed": "ts-node -r tsconfig-paths/register src/prisma/undo-seed.ts",
    "prisma:undo-seed-force": "ts-node -r tsconfig-paths/register src/prisma/undo-seed.ts --force",
    "prisma:undo-seed-advanced": "ts-node -r tsconfig-paths/register src/prisma/undo-seed-advanced.ts",
    "test:db-connection": "ts-node -r tsconfig-paths/register scripts/test-db-connection.ts",
    "clean": "rimraf node_modules package-lock.json dist",
    "clean:modules": "rimraf node_modules",
    "clean:dist": "rimraf dist",
    "clean:lock": "rimraf package-lock.json",
    "test-about-upload": "ts-node -r tsconfig-paths/register scripts/test-about-upload.ts",
    "check-about": "ts-node -r tsconfig-paths/register scripts/check-about.ts"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "prisma",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "description": "Backend application with TypeScript, Express, and Prisma",
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.17",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-queue": "^0.0.13",
    "helmet": "^8.1.0",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.3",
    "queue": "^7.0.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.22",
    "@types/supertest": "^6.0.3",
    "@types/xlsx": "^0.0.35",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.10.1",
    "prisma-dbml-generator": "^0.12.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "_moduleAliases": {
    "@": "dist",
    "src": "dist",
    "@/config": "dist/config",
    "@/middlewares": "dist/middlewares",
    "@/modules": "dist/modules",
    "@/utils": "dist/utils",
    "@/constants": "dist/constants",
    "@/types": "dist/types"
  }
}
